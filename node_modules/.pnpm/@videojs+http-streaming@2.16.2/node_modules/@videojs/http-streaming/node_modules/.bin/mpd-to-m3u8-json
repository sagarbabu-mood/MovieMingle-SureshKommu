#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/reactjs/coding-practices/spotifyRemix/node_modules/.pnpm/mpd-parser@0.22.1/node_modules/mpd-parser/bin/node_modules:/home/workspace/reactjs/coding-practices/spotifyRemix/node_modules/.pnpm/mpd-parser@0.22.1/node_modules/mpd-parser/node_modules:/home/workspace/reactjs/coding-practices/spotifyRemix/node_modules/.pnpm/mpd-parser@0.22.1/node_modules:/home/workspace/reactjs/coding-practices/spotifyRemix/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/spotifyRemix/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/spotifyRemix/node_modules/.pnpm/@videojs+http-streaming@2.16.2/node_modules/@videojs/http-streaming/node_modules:/home/workspace/reactjs/coding-practices/spotifyRemix/node_modules/.pnpm/@videojs+http-streaming@2.16.2/node_modules/@videojs/node_modules:/home/workspace/reactjs/coding-practices/spotifyRemix/node_modules/.pnpm/@videojs+http-streaming@2.16.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/reactjs/coding-practices/spotifyRemix/node_modules/.pnpm/mpd-parser@0.22.1/node_modules/mpd-parser/bin/node_modules:/home/workspace/reactjs/coding-practices/spotifyRemix/node_modules/.pnpm/mpd-parser@0.22.1/node_modules/mpd-parser/node_modules:/home/workspace/reactjs/coding-practices/spotifyRemix/node_modules/.pnpm/mpd-parser@0.22.1/node_modules:/home/workspace/reactjs/coding-practices/spotifyRemix/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/spotifyRemix/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/spotifyRemix/node_modules/.pnpm/@videojs+http-streaming@2.16.2/node_modules/@videojs/http-streaming/node_modules:/home/workspace/reactjs/coding-practices/spotifyRemix/node_modules/.pnpm/@videojs+http-streaming@2.16.2/node_modules/@videojs/node_modules:/home/workspace/reactjs/coding-practices/spotifyRemix/node_modules/.pnpm/@videojs+http-streaming@2.16.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../mpd-parser@0.22.1/node_modules/mpd-parser/bin/parse.js" "$@"
else
  exec node  "$basedir/../../../../../../mpd-parser@0.22.1/node_modules/mpd-parser/bin/parse.js" "$@"
fi
